{
  "name": "ooni-run",
  "version": "0.1.6",
  "license": "BSD-3-Clause",
  "dependencies": {
    "babel-plugin-inline-react-svg": "^0.5.2",
    "express": "^4.15.4",
    "next": "^6.0.3",
    "ooni-components": "file:.yalc/ooni-components",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "4.0.0-beta.6",
    "useragent": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/register": "7.0.0-beta.42",
    "@babel/preset-stage-0": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "babel-plugin-styled-components": "^1.5.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.2",
    "uglify-js": "^3.4.9"
  },
  "resolutions": {
    "rebass": "2.3.3",
    "styled-components": "4.0.0-beta.6",
    "styled-system": "3.1.3",
    "react-icons": "2.2.7"
  },
  "scripts": {
    "dev": "NODE_ENV=development node index.js",
    "start": "NODE_ENV=production node index.js",
    "build": "yarn run build:next && yarn run build:widgets",
    "build:next": "next build",
    "build:widgets": "browserify -t babelify widgets/index.js | uglifyjs > dist/widgets.js",
    "debug:widgets": "yarn run build:widgets && cp dist/widgets.js static/widgets.js",
    "release": "yarn run build && yarn publish --new-version $(jq -r .version package.json)",
    "genurl": "babel-node scripts/genurl.js"
  }
}
